                <attrdefs>DMPED</attrdefs>
            </attr>
            <attr>
                <attrlabl>AFFORDABLE_UNITS_AT_61_80_AMI</attrlabl>
                <attrdef>Total number of units with affordability covenants set between 61%-80% of Area Median Income. </attrdef>
                <attrdefs>DMPED</attrdefs>
            </attr>
            <attr>
                <attrlabl>AFFORDABLE_UNITS_AT_81_AMI</attrlabl>
