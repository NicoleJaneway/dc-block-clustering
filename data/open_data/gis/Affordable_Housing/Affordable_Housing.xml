            <attr>
                <attrlabl>FULLADDRESS</attrlabl>
                <attrdef>MAR Address</attrdef>
                <attrdefs>OCTO</attrdefs>
            </attr>
            <attr>
                <attrlabl>GIS_LAST_MOD_DTTM</attrlabl>
            </attr>
            <attr>
                <attrlabl>TOTAL_AFFORDABLE_UNITS</attrlabl>
                <attrdef>The total number of affordable units in the project, affordable at 80% AMI or less. This does not include the market-rate units in the project, if there were any.</attrdef>
